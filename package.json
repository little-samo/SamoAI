{
  "name": "@little-samo/samo-ai",
  "version": "0.0.1",
  "description": "The Multi-Agent AI Gaming Framework: Compete, Evolve, Adapt",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/little-samo/SamoAI.git"
  },
  "engines": {
    "node": ">=22.0.0"
  },
  "packageManager": "yarn@4.6.0",
  "main": "dist/src/index.js",
  "types": "dist/src/index.d.ts",
  "scripts": {
    "postinstall": "prisma generate",
    "clean": "rimraf dist",
    "build": "yarn clean && tsc -p tsconfig.build.json",
    "generate:prisma": "prisma generate",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "studio": "prisma studio",
    "lint": "eslint --fix",
    "test": "node --experimental-vm-modules node_modules/jest/bin/jest.js",
    "test:watch": "node --experimental-vm-modules node_modules/jest/bin/jest.js --watch",
    "test:cov": "node --experimental-vm-modules node_modules/jest/bin/jest.js --coverage",
    "test:debug": "node --experimental-vm-modules --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/jest/bin/jest.js --runInBand",
    "test:e2e": "node --experimental-vm-modules node_modules/jest/bin/jest.js --config ./test/jest-e2e.json"
  },
  "dependencies": {
    "@anthropic-ai/sdk": "^0.33.1",
    "@prisma/client": "^6.1.0",
    "dotenv": "^16.4.7",
    "ioredis": "^5.4.2",
    "mongoose": "^8.9.5",
    "openai": "^4.83.0",
    "prisma": "^6.1.0",
    "redlock": "^5.0.0-beta.2",
    "reflect-metadata": "^0.2.2",
    "zod": "^3.24.1",
    "zod-to-json-schema": "^3.24.1"
  },
  "devDependencies": {
    "@eslint/eslintrc": "^3.1.0",
    "@eslint/js": "^9.13.0",
    "@types/express": "^5.0.0",
    "@types/jest": "^29.5.2",
    "@types/node": "^20.3.1",
    "@types/supertest": "^6.0.0",
    "@typescript-eslint/eslint-plugin": "^8.0.0",
    "@typescript-eslint/parser": "^8.0.0",
    "eslint": "^9.0.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-plugin-import": "^2.31.0",
    "eslint-plugin-prettier": "^5.0.0",
    "eslint-plugin-unused-imports": "^4.1.4",
    "globals": "^15.11.0",
    "jest": "^29.5.0",
    "prettier": "^3.0.0",
    "rimraf": "^6.0.1",
    "source-map-support": "^0.5.21",
    "supertest": "^7.0.0",
    "ts-jest": "^29.1.0",
    "ts-loader": "^9.4.3",
    "ts-node": "^10.9.1",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5.7.2"
  }
}
